<?xml version="1.0"?>
<extension key="au.com.agileware.membershiputils" type="module">
  <file>membershiputils</file>
  <name>Membership Utilities</name>
  <description>Provides useful utilities for CiviCRM sites which manage memberships. See the README for more details.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Agileware</author>
    <email>support@agileware.com.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/agileware/au.com.agileware.membershiputils</url>
    <url desc="Documentation">https://github.com/agileware/au.com.agileware.membershiputils/README.md</url>
    <url desc="Support">https://agileware.com.au/contact</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-04-08</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Membershiputils</namespace>
  </civix>
</extension>
